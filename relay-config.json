{
  "gun": {
    "app-scope": "example-app-scope",
    "use-axe": false,
    "use-localstorage": false,
    "use-radisk": true,
    "use-multicast": false,
    "peers": [
      "http://localhost:3000/"
    ]
  },
  "server": {
    "port": 3000
  },
  "plugins": {
    "janitor": {
      "enabled": false,
      "max-capacity-gb": 4,
      "timer-minutes": 60
    },
    "bullet-catcher": {
      "enabled": false,
      "token": "example-token"
    }
  }
}